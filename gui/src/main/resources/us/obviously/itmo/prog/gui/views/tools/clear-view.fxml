<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="us.obviously.itmo.prog.gui.tools.controllers.ClearController"
      prefHeight="400.0" prefWidth="600.0"
      alignment="CENTER">

    <Text fx:id="errorText" fill="#F00" wrappingWidth="400" textAlignment="CENTER"/>
    <Text fx:id="infoText" wrappingWidth="400" textAlignment="CENTER"/>

    <HBox>
        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#close"
                text="Отмена">
        </Button>

        <Button fx:id="submitButton" mnemonicParsing="false" onAction="#apply"
                text="Да. Спасибо">
        </Button>
    </HBox>
</VBox>
