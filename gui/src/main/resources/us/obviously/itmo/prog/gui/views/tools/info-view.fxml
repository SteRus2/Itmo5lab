<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Label?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="us.obviously.itmo.prog.gui.tools.controllers.InfoController"
      prefHeight="400.0" prefWidth="600.0"
      alignment="CENTER">

    <Label>Count: </Label>
    <Text fx:id="countText" wrappingWidth="400" textAlignment="CENTER"/>
    <Label>Date: </Label>
    <Text fx:id="dateText" wrappingWidth="400" textAlignment="CENTER"/>
    <Label>Type: </Label>
    <Text fx:id="typeText" wrappingWidth="400" textAlignment="CENTER"/>

    <Text fx:id="errorText" fill="#F00" wrappingWidth="400" textAlignment="CENTER"/>
    <Text fx:id="infoText" wrappingWidth="400" textAlignment="CENTER"/>

    <Button fx:id="closeButton" mnemonicParsing="false" onAction="#close"
            text="Спасибо">
    </Button>
</VBox>
