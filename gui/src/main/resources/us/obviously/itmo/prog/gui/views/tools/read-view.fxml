<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.geometry.Insets?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="us.obviously.itmo.prog.gui.tools.controllers.ReadController"
      prefHeight="600.0" prefWidth="600.0"
      alignment="CENTER">

    <GridPane fx:id="grid">

    </GridPane>

    <Text fx:id="errorText" fill="#F00" wrappingWidth="400" textAlignment="CENTER"/>
    <Text fx:id="infoText" wrappingWidth="400" textAlignment="CENTER"/>

    <HBox spacing="20.0">
        <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" />
        </padding>

        <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#close"
                text="Я всё спасибо">
        </Button>
    </HBox>
</VBox>
